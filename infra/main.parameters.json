{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION=eastus}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID=}"
    },
    "openAiLocation": {
      "value": "${AZURE_OPENAI_LOCATION=eastus}"
    },
    "openAiModelName": {
      "value": "${AZURE_OPENAI_MODEL_NAME=gpt-4}"
    },
    "openAiModelVersion": {
      "value": "${AZURE_OPENAI_MODEL_VERSION=0613}"
    },
    "botDisplayName": {
      "value": "${BOT_DISPLAY_NAME=AI Agent for Teams}"
    }
  }
}
